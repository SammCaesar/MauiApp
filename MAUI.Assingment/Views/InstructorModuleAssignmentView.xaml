<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MAUI.Assingment.Views.InstructorModuleAssignmentView"
             Title="InstructorModuleAssignmentView"
             NavigatedTo="ContentPage_NavigatedTo">
    <VerticalStackLayout>
        <Label 
            Text="Add Module or Assignment"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
        <Button Text="Add Module" Clicked="AddModuleClicked"></Button>
        <Button Text="Add Assignment" Clicked="AddAssignmentClicked"></Button>
        <Button Text="Cancel" Clicked="BackButtonClicked"></Button>
    </VerticalStackLayout>
</ContentPage>